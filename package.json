{
  "name": "nunjucks-date-filter-locale",
  "version": "1.4.1",
  "description": "Date filter for nunjucks using momentjs date format functions including locale support.",
  "main": "src/index.js",
  "scripts": {
    "pretest:ci": "echo 'PRETEST' && greenkeeper-lockfile-update",
    "posttest:ci": "echo 'POSTTEST' && greenkeeper-lockfile-upload",
    "lint": "semistandard --verbose | snazzy",
    "test": "jest --coverage",
    "test:ci": "jest --coverage --ci --runInBand",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/schafevormfenster/nunjucks-date-filter-locale.git"
  },
  "tonicExampleFilename": "example/example.js",
  "keywords": [
    "nunjucks",
    "date"
  ],
  "author": "techmsi, Jan-Henrik Hempel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/techmsi/nunjucks-date/issues"
  },
  "homepage": "https://github.com/schafevormfenster/nunjucks-date-filter-locale",
  "devDependencies": {
    "commitizen": "*",
    "coveralls": "*",
    "cz-conventional-changelog": "*",
    "greenkeeper-lockfile": "^1.x",
    "jest": "*",
    "jest-junit": "*",
    "semistandard": "*",
    "snazzy": "*"
  },
  "dependencies": {
    "moment": "*",
    "nunjucks": "^3.x"
  }
}
